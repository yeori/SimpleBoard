<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ism.web.board.model.UserVO" table="users" catalog="">
        <id name="seq" type="java.lang.Integer">
            <column name="seq" />
            <generator class="identity" />
        </id>
        <property name="userId" type="string">
            <column name="userid" length="256" not-null="true" unique="true" />
        </property>
        <property name="nickName" type="string">
            <column name="nickname" length="20" not-null="true" unique="true" />
        </property>
        <property name="email" type="string"></property>
        <property name="password" type="string"></property>
        <property name="whenJoined" type="string" column="when_joined"></property>
        <!-- <property name="" type="string"></property>
        <property name="" type="string"></property> -->
    </class>
</hibernate-mapping>